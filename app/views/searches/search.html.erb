<h1>Search for Coffee Shops Near Me</h1>
<%= form_tag '/search' do %>
  <%= label_tag :zipcode %>
  <%= text_field_tag :zipcode %>
  <%= submit_tag "Search!" %>
<% end %>

<div>
  <% if @venues%>
    <% @venues.each do |v| %>
      <%= v['name']%> <br>
    <% end %>
  <% end %>
</div>

<div>
  <% if @error %>
    Something's gone horribly wrong! <br>
    <%= @error %>
  <% end %>
</div>
