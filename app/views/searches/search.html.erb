<h1>Search for Coffee Shops Near Me</h1>
<%= form_tag '/search' do %>
  <%= label_tag :zipcode %>
  <%= text_field_tag :zipcode %>
  <%= submit_tag "Search!" %>
<% end %>
<div>
  <% if @error %>
    <p><%= @error %></p>
  <% elsif @venues %>
  <ul>
    <% @venues.each do |v| %>
      <li>
        <%= v["name"] %><br>
        <%= v["location"]["address"] %><br>
        Checkins: <%= v["stats"]["checkinsCount"] %>
      </li>
    <% end %>
  </ul>
  <% end %>
</div>
