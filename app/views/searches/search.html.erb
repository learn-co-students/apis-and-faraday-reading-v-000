<h1>Search for Coffee Shops Near Me</h1>
<%= form_tag '/search' do %>
  <%= label_tag :zipcode %>
  <%= text_field_tag :zipcode %>
  <%= submit_tag "Search!" %>
<% end %>
<div>
  <% if @error %>
    <p><%= @error %></p>
    <% elsif @venues %>
    <ul>
      <% @venues.each do |venue| %>
      <li>
        <p>Name: <%= venue["name"]%><br></p>
        <p>Address: <%= venue["location"]["address"]%></p>
        <% if venue["location"]["crossStreet"]%>
        <p>Cross Street: <%= venue["location"]["crossStreet"]%></p>
        <% end %>
      </li>
      <% end %>
    </ul>
    <% end %>
</div>
