<h1>Search for Coffee Shops Near Me</h1>
<%= form_tag '/search' do %>
  <%= label_tag :zipcode %>
  <%= text_field_tag :zipcode %>
  <%= submit_tag "Search!" %>
<% end %>
<div>
  <% if @error %>
    <%= @error %>
  <% elsif @venues %>
  <ul>
    <% @venues.each do |venue| %>
    <li>
      <%= venue["name"] %>
      <%= venue["location"] %>
      <%= venue["address"] %>
      <%= venue["stats"]["checkinsCount"] %>
    </li>
    <% end %>
  </ul>
  <% end %>
</div>